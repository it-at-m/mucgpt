# Core
MUCGPT_CORE_MODELS='[{"type": "OPENAI", "llm_name": "<your-llm-name>",  "endpoint": "<your-endpoint>", "api_key": "<your-sk>", "model_info": {"auto_enrich_from_model_info_endpoint": true, "max_output_tokens": "<number>", "max_input_tokens": "<number>", "description": "<description>", "knowledge_cut_off": "2024-07-01", "input_cost_per_token": "<price>", "output_cost_per_token": "<price>", "supports_function_calling": true, "supports_reasoning": false, "supports_vision": true, "litellm_provider": "<provider>", "inference_location": "<region>"}}]'
